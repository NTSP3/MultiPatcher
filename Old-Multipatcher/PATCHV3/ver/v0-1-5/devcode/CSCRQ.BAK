int main(int argc, char* argv[]) {

  if (argc > 1 && strcmp(argv[1], "/runbymain:run-with-custom-settings") == 0) {

    if (argc > 2) {

      if (strcmp(argv[2], "/scr::1") == 0) {

        numsvar = '1';

      }

      else if (strcmp(argv[2], "/scr::2") == 0) {

        numsvar = '2';

      }

      else if (strcmp(argv[2], "/scr::2+hg7") == 0) {

        numsvar = '2';
        colsbar = '7';

      }

      else {

        printf("\nSupplementary program developed for MultiPatcher.\r\n");

      }

      if (argc > 3) {

        if (strcmp(argv[3], "/true") == 0) {

          denyani = '1';

        }

        if (argc > 4) {

          if (strcmp(argv[4], "/true") == 0) {

            nocolor = '1';

          }

          if (argc > 5) {

            if (strcmp(argv[5], "/true") == 0) {

              fbfmode = '1';
              UserQuitCheck();

            }

          }

          else QuitAnimation();

        }

        else QuitAnimation();

      }

      else QuitAnimation();

    }

    else {

      printf("\nSupplementary program developed for MultiPatcher.\r\n");

    }

  }

  else {

    printf("\nSupplementary program developed for MultiPatcher.\r\n");

  }

  return 0;

}

